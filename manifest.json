{
    "manifest_version": 3,
    "name": "Egret Inspector",
    "description": "Egret Chrome Debug Tool",
    "version": "3.0.0",
    "devtools_page": "devtools.html",
    "key": "",
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "js": [
                "contentScripts.min.js"
            ],
            "run_at": "document_end"
        }
    ],
    "background": {
        "service_worker": "backgroudScripts.min.js"
    },
    "host_permissions": [
        "*://*/*"
    ],
    "permissions": [
        "system.cpu",
        "tabs",
        "storage",
        "nativeMessaging"
    ],
    "web_accessible_resources": [{
        "resources": [
            "injectScripts.min.js",
            "ipt/shared/*",
            "ipt/inject/*"
        ],
        "matches": ["<all_urls>"]
    }],
    "icons": {
        "48": "icon.png"
    }
}
